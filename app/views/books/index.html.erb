<h1>Books#index</h1>
<%= search_form_for @q do |f| %>

  <%= f.label :name_or_description_cont %>
  <%= f.search_field :name_or_description_cont %>

  <%= f.collection_select :catagory_id_eq, Catagory.all, :id, :name, include_blank: true %>



  <%= f.submit %>

<% end %>
<table>
  <%= render @books %>
</table>

<%= paginate @books %>
